{
  "name": "npm-hax",
  "version": "0.2.0",
  "description": "Blacklist a package.json dependency entry at any depth in your dependency tree, making it invisible to npm install and other npm commands",
  "homepage": "https://github.com/ludios/npm-hax",
  "issues": "https://github.com/ludios/npm-hax/issues",
  "keywords": [
    "npm",
    "dependency",
    "dependencies"
  ],
  "main": "cli.js",
  "preferGlobal": true,
  "bin": {
    "npm-hax": "./cli.js"
  },
  "author": "Ivan Kozik",
  "license": "ISC",
  "dependencies": {
    "read-package-json": "^2.0.0",
    "which": "^1.1.1"
  },
  "repository": "ludios/npm-hax",
  "engines": {
    "iojs": ">= 2.0.0",
    "node": ">= 4.0.0"
  }
}
